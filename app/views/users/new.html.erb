<h1>
Create New User
</h1>

<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<%= f.label :favorite_fortune %>
<%= f.text_field :favorite_fortune %>
<%= f.submit %>
<% end %>
